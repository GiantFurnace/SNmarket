<template name="ad-swiper">
  <view class="swiper-container">
    <swiper indicator-dots="{{false}}" autoplay="{{true}}" interval="5000" duration="1000">
      <block wx:for="{{imgUrls}}" wx:key="{{key}}" wx:for-item="item">
        <swiper-item>
          <image src="{{item}}" style="width:98%;height:185rpx;"></image>
        </swiper-item>
      </block>
    </swiper>
  </view>
</template>